{

    "manifest_version":2,
    "name":"Illegal Services Bookmarks",
    "version":"0.0.0.2",
    "description":"Create and update Illegal Services indexed websites in a single bookmark.",
    "author":"IB_U_Z_Z_A_R_Dl",
    "background":{
        "scripts":["background.js"],
        "persistent":false
    },
    "permissions":[
        "bookmarks",
        "https://github.com/",
        "https://*.amazonaws.com/"
    ],
    "icons":{
        "16":"icon_16x16.png",
        "32":"icon_32x32.png",
        "48":"icon_48x48.png",
        "64":"icon_64x64.png",
        "128":"icon_128x128.png"
    },
    "browser_action":{
        "default_icon":{
            "16":"icon_16x16.png",
            "32":"icon_32x32.png",
            "48":"icon_48x48.png",
            "64":"icon_64x64.png",
            "128":"icon_128x128.png"
        },
        "default_title":"Illegal Services Bookmarks",
        "default_popup":"popup.html"
    }

}